@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

# Capture values from the configure stage
set(USE_INTERNAL_ALL @USE_INTERNAL_ALL@)
set(USE_EXTERNAL_BETTER_ENUMS @USE_EXTERNAL_BETTER_ENUMS@)
set(USE_EXTERNAL_CXXOPTS @USE_EXTERNAL_CXXOPTS@)
set(USE_EXTERNAL_FMT @USE_EXTERNAL_FMT@)
set(USE_EXTERNAL_TOML11 @USE_EXTERNAL_TOML11@)
set(USE_EXTERNAL_TINYXML2 @USE_EXTERNAL_TINYXML2@)

set(BUILD_SHARED_LIBS @BUILD_SHARED_LIBS@)
set(ENABLE_DEBUG @ENABLE_DEBUG@)
set(ENABLE_IPO @ENABLE_IPO@)
set(ENABLE_PROFILING @ENABLE_PROFILING@)
set(ENABLE_TESTS @ENABLE_TESTS@)
set(ANTMOC_ENABLE_COVERAGE @ANTMOC_ENABLE_COVERAGE@)

set(ENABLE_OPENMP @ENABLE_OPENMP@)
set(ENABLE_MPI @ENABLE_MPI@)
set(ENABLE_DOUBLE_PRECISION @ENABLE_DOUBLE_PRECISION@)
set(ENABLE_CYCLIC @ENABLE_CYCLIC@)
set(ENABLE_CMFD @ENABLE_CMFD@)
set(ENABLE_LINEAR_SOURCE @ENABLE_LINEAR_SOURCE@)

find_dependency(OpenMP @OPENMP_VERSION@)
find_dependency(HDF5 @HDF5_VERSION@ EXACT)
if (ENABLE_MPI)
    find_dependency(MPI @MPI_CXX_VERSION@)
endif ()

if (USE_EXTERNAL_CXXOPTS)
    find_dependency(cxxopts)
endif ()

if (USE_EXTERNAL_TOML11)
    find_dependency(toml11 @TOML11_VERSION@ EXACT)
endif ()

if (USE_EXTERNAL_FMT)
    find_dependency(FMT @FMT_VERSION@ EXACT)
endif ()

if (USE_EXTERNAL_TINYXML2)
    find_dependency(tinyxml2 @TINYXML2_VERSION@ EXACT)
endif ()

# Add the targets file
include("${CMAKE_CURRENT_LIST_DIR}/antmocTargets.cmake")
